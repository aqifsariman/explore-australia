<%- include('header') -%>
  <h1 class="text-center">Favorites</h1>
  <div class=" my-auto d-flex align-items-center justify-content-center trip-bg">
    <div class="col-sm-12 mt-5">
      <div class="container">
        <% finalResults.forEach((attraction)=>{ %>
      <p class="text-light fs-2 my-3 fw-bolder"><%= attraction.attractionsname %> <span> - <%= attraction.name %></span></p>
      <div class="d-flex flex-wrap justify-content-start align-items-center my-3 mx-auto row">
      <img src="<%= attraction.imagelink %>" class="img-fluid attraction-img "> 
      <div class="col">
      <p class="text-light col-xs-4 fs-3 col-xl-6 mx-3 text-start"> <%= attraction.details %></p>
      </div>
      </div>
     <% }); %>
  </div>
  </div>
  </div>
<%- include('footer') -%>